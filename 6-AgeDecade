public class AgeDecade {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        int age1=23;
        int decade=age1/10;
        decade=decade+1;
        System.out.println("You are: "+age1+" years old, and you are in decade: "+decade);
        
                }
    
    
}
