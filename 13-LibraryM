import java.util.Scanner;
public class KhuloodAbdulmajeed_LibraryM {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        Scanner get= new Scanner (System.in); 
        System.out.println("Enter number of borrowed books:");
        int BorrowedBooks = get.nextInt();
        System.out.println("Enter Your membership duration:");
        int Duration = get.nextInt();
         
        if (Duration>3 && BorrowedBooks>10){
            System.out.println("Your membership is Golden");
            
        }else if (Duration>3 && BorrowedBooks<=10){
            System.out.println("Your membership is Silver");
                    }else if (Duration<=3 && BorrowedBooks>10){
                        System.out.println("Your membership is Bronze");
                    }else { System.out.println("No library membership");}
                    
                    }
    }
    

