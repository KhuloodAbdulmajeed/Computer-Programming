import java.util.Scanner;
public class KhuloodAbdulmajeed_Factorial {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        Scanner get=new Scanner(System.in);
        int StartingNum;
        int EndingNum;
        int Range;
        int Factorail=1;
     
        
        System.out.print("Enter the starting number: ");
        StartingNum=get.nextInt();
        System.out.print("Enter the ending number: ");
        EndingNum=get.nextInt();
        
        while(EndingNum<=StartingNum){
            System.out.println("Wrong range of values");
          
            break;}
       
        while (EndingNum>StartingNum){
        Range=EndingNum;
        
        for(int i=StartingNum;StartingNum<=EndingNum;++i){
            Factorail*=i;
            
            System.out.println("The factorial of "+StartingNum+" = "+Factorail);  
            ++StartingNum;
        }
        
        System.out.println("The range of this set is: "+Range);

        }




        }
        
    }
   
